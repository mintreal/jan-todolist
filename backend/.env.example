# 데이터베이스 연결 URL
DATABASE_URL="postgresql://user:password@host:port/database"

# JWT 시크릿 키
JWT_SECRET="your-jwt-secret"

# JWT 토큰 만료 시간 (예: 7d, 24h, 60m)
JWT_EXPIRES_IN="7d"

# 서버 포트
PORT=3000

# 실행 환경 (development, production, test)
NODE_ENV="development"

# CORS 허용 오리진
CORS_ORIGIN="http://localhost:5173"
